<table class="table table-hover">
	<thead>
		<tr>
			<th width="30"><input type="checkbox" rel="#category-trash-btn" /></th>
			<th>认证名称</th>
			<th>图标</th>
			<th width="50">描述</th>
			<th width="50">状态</th>
			<th width="30"></th>
		</tr>
	</thead>
	<tbody>
		{% for ident in identList %}
		<tr cat-id="{{ident._id}}">
			<td width="10px"><input type="checkbox" name="name" value="{{ident._id}}" /></td>
			<td width="170px">{{ident.name}}</td>
			<td width="80px"><img src="{{ident.icon}}" style="max-width:50px;"/></td>
			<td width="150px">{{ident.desc}}</td>
			<td>{{ident.status|status}}</td>
			<td class="table-action">
				<a class="fa icon-edit" app-bubble="false"></a>
			</td>
		</tr>
		{% endfor %}
	</tbody>
</table>
